{
   "name": "game_on",
   "version": "1.0.0",
   "description": "Backend API for Game On",
   "main": "index.js",
   "author": "Jose Diaz",
   "license": "MIT",
   "private": true,
   "scripts": {
      "start:server": "ts-node-dev --respawn --poll ./main.ts",
      "test": "lab -v **/*.test.js",
      "start": "ts-node src/index.ts",
      "create": "node tasks/createDB.js",
      "test:create": "node tasks/createDB.js test",
      "drop": "node tasks/dropDB.js",
      "drop:all": "(node tasks/dropDB.js) && (node tasks/dropDB.js test)",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
   },
   "dependencies": {
      "@hapi/code": "^8.0.1",
      "@hapi/hapi": "^19.1.1",
      "@hapi/lab": "^22.0.4",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.24",
      "typescript": "^3.8.3"
   },
   "devDependencies": {
      "ts-node-dev": "^1.0.0-pre.44",
      "ts-node": "3.3.0",
      "@types/node": "^8.0.29"
   }
}
