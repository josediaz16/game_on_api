{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"]
      },
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   }
}
